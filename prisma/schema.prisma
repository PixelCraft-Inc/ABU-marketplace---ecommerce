datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id
  email     String   @unique
  name      String?
  lastName  String?
  image     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
